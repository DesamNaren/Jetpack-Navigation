<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.cgg.navjetpackapp.MainFragment"
        android:label="HomeFragment" >
        <argument
            android:name="NAME"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/categoriesFragment"
        android:name="com.cgg.navjetpackapp.CategoriesFragment"
        android:label="CategoriesFragment" >
        <action
            android:id="@+id/action_categoriesFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_categoriesFragment_to_cartFragment"
            app:destination="@id/cartFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.cgg.navjetpackapp.SearchFragment"
        android:label="SearchFragment" />
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.cgg.navjetpackapp.CartFragment"
        android:label="CartFragment" />
</navigation>